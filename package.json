    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "typescript": "^5.9.3"
  }
}
